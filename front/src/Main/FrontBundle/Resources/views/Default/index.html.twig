{% block head_style %}
	{% block stylesheets %}
			<meta http-equiv="X-UA-Compatible" content="IE=edge">

      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/site.min.css') }}" />
      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/about/creative.css') }}" />
      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/about/font-awesome.min.css') }}" />
	{% endblock %}
{% endblock head_style %}

{% block head_script %}

<!--[if lt IE 9]>
  {% javascripts '@MainFrontBundle/Resources/public/js/library/respond.min.js'
  				 '@MainFrontBundle/Resources/public/js/library/html5shiv.min.js' %}
  	<script src="{{ asset_url }}"></script>
  {% endjavascripts %}
<![endif]-->

{% javascripts 
		'@MainFrontBundle/Resources/public/js/library/site.min.js'
		'@MainFrontBundle/Resources/public/js/about/jquery.fittext.js'
        '@MainFrontBundle/Resources/public/js/about/jquery.easing.min.js'
        '@MainFrontBundle/Resources/public/js/about/jquery.fittext.js'
        '@MainFrontBundle/Resources/public/js/about/wow.min.js'
        '@MainFrontBundle/Resources/public/js/about/creative.js'%}
    <script type="text/javascript">
      var base = location.protocol + "//" + location.host + '{{ app.request.baseUrl }}';
  </script> 
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}

<script>
$(function () {
  var nua = navigator.userAgent
  var isAndroid = (nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 && nua.indexOf('AppleWebKit') > -1 && nua.indexOf('Chrome') === -1)
  if (isAndroid) {
    $('select.form-control').removeClass('form-control').css('width', '100%')
  }
})
</script>
<title>{% block title %}{% trans %}about.section1.h2{% endtrans %}{% endblock %}</title>
{% block more_script %}

{% endblock more_script %}     

{% endblock head_script %} 



{% block container %}
    {% include "MainFrontBundle:Menu:menu.html.twig" %}
    <section class="bg-primary-index" id="about">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-lg-6 text-center">
                	<h2 class="section-heading">{% trans %}about.section1.h2{% endtrans %}</h2>
                    <p class="text-fade">
                    	<strong>
                    	{% trans %}about.section1.p{% endtrans %}
                    	</strong>
                    </p>
                     {{ form_start(form) }}
	   				<div class="form-group {% if not form.category.vars.valid %}has-error{% endif %}">
	   	   				{{ form_widget(form.category) }}
	   				</div>
				  	 <button type="submit" class="btn btn-default btn-xl">
				  	 	{% trans %}search.index.btn.submit{% endtrans %}
				  	 </button>
				  	 {{ form_rest(form) }}
				   {{ form_end(form) }}
                </div>
            </div>
        </div>
    </section>

    <section id="services">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-lg-12 text-center">
                    <h2 class="section-heading">{% trans %}about.section2.h2{% endtrans %}</h2>
                    <hr class="primary">
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="service-box">
                        <i class="fa fa-4x fa-camera wow bounceIn text-primary"></i>
                        <h3>{% trans %}about.section2.camera.h3{% endtrans %}</h3>
                        <p class="text-muted">{% trans %}about.section2.camera.text{% endtrans %}</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="service-box">
                        <i class="fa fa-4x fa-heart wow bounceIn text-primary" data-wow-delay=".3s"></i>
                        <h3>{% trans %}about.section2.heart.h3{% endtrans %}</h3>
                        <p class="text-muted">{% trans %}about.section2.heart.text{% endtrans %}</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="service-box">
                        <i class="fa fa-4x fa-eur wow bounceIn text-primary" data-wow-delay=".1s"></i>
                        <h3>{% trans %}about.section2.eur.h3{% endtrans %}</h3>
                        <p class="text-muted">{% trans %}about.section2.eur.text{% endtrans %}</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="service-box">
                        <i class="fa fa-4x fa-star wow bounceIn text-primary" data-wow-delay=".2s"></i>
                        <h3>{% trans %}about.section2.star.h3{% endtrans %}</h3>
                        <p class="text-muted">{% trans %}about.section2.star.text{% endtrans %}</p>
                    </div>
                </div>
                
            </div>
        </div>
    </section>
	{% include "MainFrontBundle:Menu:footer.html.twig" %}
     {% block bottom_script %}

     {% endblock bottom_script %}
     
{% endblock container %}


