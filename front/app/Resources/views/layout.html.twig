{% block head_style %}
	{% block stylesheets %}
			<meta http-equiv="X-UA-Compatible" content="IE=edge">
        <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/ui-lightness/jquery-ui-1.10.4.custom.css') }}" type="text/css" media="all" />
			<link rel="stylesheet" href="{{ asset('bundles/mainfront/css/bootstrap.min.css') }}" />
      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/datepicker.css') }}" />
      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/site.min.css') }}" />
      <link rel="stylesheet" href="{{ asset('bundles/mainfront/css/star-rating.min.css') }}" />
	{% endblock %}
{% endblock head_style %}

{% block head_script %}

<!--[if lt IE 9]>
  {% javascripts '@MainFrontBundle/Resources/public/js/library/respond.min.js'
