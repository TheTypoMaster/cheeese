{% extends '::base.html.twig'%}
{% block titleH1 %}{% trans from "users" %}gestion.security.read.h1{% endtrans %}{% endblock %}
{% block titleH2 %}
{% trans from "users" %}gestion.security.read.h2{% endtrans %}
{% endblock %}
{% block breadcrumbH1 %}{% trans from "users" %}gestion.security.read.breadh1{% endtrans %}{% endblock %}
{% block breadcrumbH2 %}
{% trans from "users" %}gestion.security.read.breadh2{% endtrans %}
{% endblock %}
{% block content %}
<section class="content">
	<div class="row">
			<div class="col-md-12">
                            <div class="box box-primary">
                                <div class="box-header">
                                    <h3 class="box-title">
                                    	{% trans from "users" %}gestion.security.read.header {% endtrans %}
                                    </h3>
                                </div>
                               {{ form_start(form, {attr: {novalidate: 'novalidate'}}) }}
                                    <div class="box-body">									
                                        {{ form_rest(form) }}
                                    </div>

                                    <div class="box-footer">
                                        <button type="submit" class="btn btn-primary">
                                        	{% trans from "users" %}gestion.security.read.btn.submit{% endtrans %}
                                        </button>
                                    </div>
                                {{ form_end(form) }}
                            </div>
		</div>	
	</div>
</section>
{% endblock %}