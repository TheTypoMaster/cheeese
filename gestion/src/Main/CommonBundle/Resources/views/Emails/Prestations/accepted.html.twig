{% embed "MainCommonBundle:Emails:email.html.twig" %}
		{% block h1 %}{{ prestation.devis.company.photographer.username }},{% endblock %}
		{% block p_lead %}
			{% trans with{'%reference%' : prestation.reference} from "email" %}prestation.accepted.p_lead %reference%
			{% endtrans %}
		{% endblock %}
		{% block p1 %}
			{% trans from "email" %}prestation.accepted.p1
			{% endtrans %}
		{% endblock %}
		{% block p2 %}
			{% trans from "email" %}prestation.accepted.p2
			{% endtrans %}
		{% endblock %}
		{% block panel %}
			{% trans from "email" %}prestation.accepted.panel
			{% endtrans %}
		{% endblock %}
		{% block link %}{{ base_url}}/service/{{ prestation.id }}{% endblock %}
		{% block text %}
			{% trans from "email" %}prestation.accepted.text
			{% endtrans %}
		{% endblock %}

		{% block mail %}
			{% trans from "email" %}generic.mail.photographer
			{% endtrans %}
		{% endblock %}


	{% endembed %}