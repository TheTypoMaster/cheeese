{% if 'ROLE_PHOTOGRAPHER' == role %}
    {% if null == company %}
		<div class="alert alert-info alert-dismissable">
		<i class="fa fa-info"></i>
		<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
		<b>{% trans from "default" %}community.default.headband.info.banner{% endtrans %}</b>
		{% trans from "default" %}community.default.headband.info.message{% endtrans %}
		</div>
	{% elseif 1 == company.status.id %}
		<div class="alert alert-warning alert-dismissable">
		<i class="fa fa-warning"></i>
		<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
		<b>{% trans from "default" %}community.default.headband.warning.banner{% endtrans %}</b>
		{% trans from "default" %}community.default.headband.warning.message{% endtrans %}
		</div>
	{% elseif 3 == company.status.id %}
		<div class="alert alert-danger alert-dismissable">
		<i class="fa fa-ban"></i>
		<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
		<b>{% trans from "default" %}community.default.headband.danger.banner{% endtrans %}</b>
		{% trans from "default" %}community.default.headband.danger.message{% endtrans %}
		</div>	
	{% endif %}
{% endif %}
